{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/jobs/:id",
      "destination": "/api/job-page?id=:id"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "aeroscout.net"
        }
      ],
      "destination": "https://www.aeroscout.net/:path*",
      "permanent": true
    }
  ]
}
